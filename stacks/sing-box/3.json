{
			"type": 3,
			"title": "sing-box",
	 		"name": "01_sing-box",
			"description": "一键安装sing-box网关",
			"note": "通过docker容器,配合macvlan,快速使用BOX打造S5|SS多个网关.然后通过爱快下一跳网关进行分流 更多<a href=\"https://hub.docker.com/r/lihaixin/sing-box\" target=\"_blank\">点击这里</a>",
			"categories": ["route"],
			"platform": "linux",
			"logo": "https://dockerfile.15099.net/logo/sing-box.png",
			"repository": {
				"url": "https://gh.15099.net/https://github.com/lihaixin/dockerfile",
				"stackfile": "stacks/sing-box/docker-compose.yml"
			},
	 		"env": [
				{
				"name": "USERNO",
				"label": "用户授权USERNO",
				"description": "用户授权USERNO,也是自定义模板标识号,未授权USERNO只能使用免费模板",
				"default": "9999210012301280002",
				"preset": true
				},
        		{
				"name": "DNS_ADDR",
				"label": "落地DNS解析信息",
				"description": "请选择合适的版本，国内IP选择国内、国外IP选择国外",
				"select": [
				        {
				          "text": "国外",
				          "value": "8.8.8.8",
				          "default": true
				        },
				        {
				          "text": "国内",
				          "value": "223.6.6.6"
				        }
				      ]
				},
				{
				"name": "TYPE",
				"label": "代理类别",
				"description": "请选择合适的版本，socks5 | ss | ss_head | wireguard | wireguard_preshare | vless_reality_tcp | tuic",
				"select": [
				        {
				          "text": "socks5普通代理",
				          "value": "client_socks5",
				          "default": true
				        },
				        {
				          "text": "shadowsocks加密代理",
				          "value": "client_ss"
				        },
				        {
				          "text": "shadowsocks_head加密代理",
				          "value": "client_ss_head"
				        },
				        {
				          "text": "wireguard_隧道",
				          "value": "client_wireguard"
				        },
				        {
				          "text": "wireguard_preshare_隧道",
				          "value": "client_wireguard_preshare"
				        },
				        {
				          "text": "vless_reality_tcp上网冲浪",
				          "value": "client_vless_grpc"
				        },
				        {
				          "text": "tuic游戏专项",
				          "value": "client_tuic"
				        }
				      ]
				},
				{
				"name": "SERVER",
				"label": "落地代理IP",
				"description": "输入落地IP",
				"default": "6.6.6.6"
				},
				{
				"name": "PORT",
				"label": "落地代理端口",
				"description": "输入落地IP端口",
				"default": "1080"
				},
				{
				"name": "USER",
				"label": "落地代理账号|SS加密方式|WG服务端公钥|Vless UUID| tuic UUID",
				"description": "落地代理账号 | SS加密方式：xchacha20-ietf-poly1305  2022-blake3-aes-128-gcm | WG服务端公钥 |Vless UUID| tuic UUID",
				"default": "user"
				},
				{
				"name": "PASSWD",
				"label": "落地代理密码| WGK客户端私钥| Vless UUID | tuic UUID",
				"description": "落地socks5密码 | ss密码 | WGK客户端私钥 | vless 密码 | tuic 密码",
				"default": "password"
				},
				{
				"name": "WGADDR",
				"label": "wireguard服务器端地址",
				"description": "用于客户端和服务器通信的内网地址",
				"default": "10.8.1.11/24"
				},
				{
				"name": "PRE_KEY",
				"label": "wireguard 预共享密钥",
				"description": "是一个可选的额外加密层，用于增强安全性",
				"default": "Hj8kLm9nOp0qRs1tUv2wXy3zAb4cDe5fGh6iJk7lMn8="
				},
				{
				"name": "IP",
				"label": "容器macvlan IP *",
				"description": "修改不同的IP",
				"default": "172.19.0.1"
				}
				]
}
,
