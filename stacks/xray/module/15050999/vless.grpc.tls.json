      {
      // 5.1 默认转发VPS,需要调整出口只要调整 5.1
      // 一定放在第一个，在routing 3.5 里面已经说明了，这等于是默认规则，所有不符合任何规则的流量都走这个
       "tag": "$OUTBOUNDTAG",
       "protocol": "vless",
       "settings": {
          "vnext": [
                    {
                        "address": "$RDOMAIN",
                        "port": $RPORT,  //  http2的默认端口443 https://quic.nginx.org/readme.html
                        "users": [
                            {
                                "id": "$UUID",  //填写你的 UUID
				"encryption":"none",
				"security": "auto"
                            }
                        ]
                    }
                ]
         },
	 "mux": {
		"enabled": true,
  		"concurrency": -1
        },
       "streamSettings": {
		 "network": "grpc",
      		 "security": "tls",
                 "grpcSettings": {
                 	"servername": "grpc.qq.com" //填写你的 ServiceName
       		}
     	}
    }
    ,
