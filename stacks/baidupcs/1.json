{
    			"type": 1,
    			"title": "baidupcs-1999",
    			"description": "不限速百度云客户端",
    			"categories": ["file"],
    			"platform": "linux",
    			"logo": "https://raw.githubusercontent.com/lihaixin/dockerfile/master/logo/baidupcs.png",
    			"image": "lihaixin/baidupcs",
    			"ports": [
      				"1999/tcp"
    				],
    			"volumes": [{
				"bind": "/defaults",
				"container": "/defaults"
				},
				{
				"bind": "/root/Downloads",
				"container": "/root/Downloads"
				}],
    			"env": [{
      					"name": "PGID",
      					"label": "PGID",
      					"default": "0"
    				},
    				{
      					"name": "PUID",
      					"label": "PUID",
      					"default": "0"
    				}],
    			"restart_policy": "always"
  			}
,
