{
	"version": "2",
	"templates": [{
			"type": 1,
			"title": "Registry",
			"description": "Docker image registry",
			"categories": ["docker"],
			"platform": "linux",
			"logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/registry.png",
			"image": "registry:latest",
			"ports": [
				"5000/tcp"
				],
			"volumes": [{
				"container": "/var/lib/registry"
				}]
			},
			{
   		 	"type": 1,
    			"title": "webssh2",
    			"description": "Web SSH Client using WebSSH2",
    			"categories": ["ssh"],
   			"platform": "linux",
			"restart_policy": "always",
    			"logo": "https://raw.githubusercontent.com/lihaixin/dockerfile/master/logo/webssh2.png",
    			"image": "lihaixin/webssh2:latest",
    			"ports": [
      				"2222/tcp"
    				]
  			},
		      	{
    			"type": 1,
    			"title": "ttyd",
    			"description": "ttyd is a simple command line tool for sharing terminal over the web ",
    			"categories": ["ssh"],
    			"platform": "linux",
			"restart_policy": "always",
    			"logo": "https://raw.githubusercontent.com/lihaixin/dockerfile/master/logo/ttyd.png",
    			"image": "lihaixin/ttyd",
    			"ports": [
      				"7681/tcp"
    				],
			"command": "ttyd -c root:password -P 3600 bash"
  			},
		      	{
    			"type": 1,
    			"title": "next-terminal",
    			"description": "Next Terminal是使用Golang和React开发的一款HTML5的远程桌面网关，具有小巧、易安装、易使用、资源占用小的特点，支持RDP、SSH、VNC和Telnet协议的连接和管理 ",
    			"categories": ["ssh"],
    			"platform": "linux",
			"restart_policy": "always",
    			"logo": "https://raw.githubusercontent.com/lihaixin/dockerfile/master/logo/Next-Terminal.png",
    			"image": "dushixiang/next-terminal:0.3.4",
    			"ports": [
      				"8088/tcp"
    				],
			"volumes": [{
				"container": "/usr/local/next-terminal"
				}]
			},
 			{
    			"type": 1,
    			"title": "tinyportmapper",
    			"description": "TCP UDP端口转发",
    			"categories": ["tools"],
    			"platform": "linux",
    			"logo": "https://raw.githubusercontent.com/lihaixin/dockerfile/master/logo/tinyportmapper.png",
    			"image": "lihaixin/tinyportmapper:latest",
    			"ports": [
	      			"5001/tcp",
      				"5001/udp"
    				]
			},
 			{
    			"type": 1,
    			"title": "baidupcs",
    			"description": "不限速百度云客户端",
    			"categories": ["file"],
    			"platform": "linux",
    			"logo": "https://raw.githubusercontent.com/lihaixin/dockerfile/master/logo/baidupcs.png",
    			"image": "lihaixin/baidupcs",
    			"ports": [
      				"1999/tcp"
    				],
    			"volumes": [{
				"container": "/defaults"
				},
				{
				"container": "/root/Downloads"
				}],
    			"env": [{
      					"name": "PGID",
      					"label": "PGID",
      					"set": "0"
    				},
    				{
      					"name": "PUID",
      					"label": "PUID",
      					"set": "0"
    				}],
    			"restart_policy": "always"
  			},
   			{
    			"type": 1,
    			"title": "h5ai",
    			"description": "h5ai file indexer for HTTP web servers",
   			"categories": ["file"],
    			"platform": "linux",
    			"logo": "https://raw.githubusercontent.com/lihaixin/dockerfile/master/logo/h5ai.png",
    			"image": "lihaixin/h5ai",
    			"ports": [
      				"80/tcp"
    				],
    			"restart_policy": "always",
     			"volumes": ["/share"]
  			},
  			{
    			"type": "container",
    			"title": "h5ai auth",
    			"description": "h5ai file indexer for HTTP web servers",
    			"categories": ["file"],
    			"platform": "linux",
    			"logo": "https://raw.githubusercontent.com/lihaixin/dockerfile/master/logo/h5ai.png",
    			"image": "lihaixin/h5ai:auth",
    			"ports": [
      				"80/tcp"
    				],
     			"env": [{
      				"name": "USER",
      				"label": "USER",
      				"set": "test"
    				},
     				{
      				"name": "PASSWD",
      				"label": "PASSWD",
      				"set": "test"
    				}],
    			"restart_policy": "always",
     			"volumes": ["/share"]
  			},
	]
}
                
